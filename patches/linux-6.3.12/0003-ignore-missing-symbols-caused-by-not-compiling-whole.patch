diff --git a/scripts/mod/modpost.c b/scripts/mod/modpost.c
index dd0419e3ec07d92ce4c305126a556d7c7cc7043f..3aacdc8985046aa35ba1f177444a9d299c9f78c2 100644
--- a/scripts/mod/modpost.c
+++ b/scripts/mod/modpost.c
@@ -1919,6 +1919,11 @@ static void check_exports(struct module *mod)
 		const char *basename;
 		exp = find_symbol(s->name);
 		if (!exp) {
