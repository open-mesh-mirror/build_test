diff --git a/scripts/mod/modpost.c b/scripts/mod/modpost.c
index d5d8562b01ea599c0be5c14a708ae649cecda73e..f3ac60e5cf3034e917fa7478279960ad1cb7f1b6 100644
--- a/scripts/mod/modpost.c
+++ b/scripts/mod/modpost.c
@@ -1748,6 +1748,11 @@ static void check_exports(struct module *mod)
 		const char *basename;
 		exp = find_symbol(s->name);
