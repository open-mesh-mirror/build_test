diff --git a/scripts/mod/modpost.c b/scripts/mod/modpost.c
index d31a70c3fa822214d50b5bcf1d2f70c9dd0494a8..a695bc0f2bd525b1fa341ff1212f133e6dfb2c94 100644
--- a/scripts/mod/modpost.c
+++ b/scripts/mod/modpost.c
@@ -2120,6 +2120,11 @@ static void check_exports(struct module *mod)
 		const char *basename;
 		exp = find_symbol(s->name);
 		if (!exp || exp->module == mod) {
+			/* ignore the unresolved symbols caused by only using
