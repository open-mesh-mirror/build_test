diff --git a/scripts/mod/modpost.c b/scripts/mod/modpost.c
index 5fa05caa8507fb54eb94620b8e7139168f1bb35d..f0d4d1e5289004bbdd2a258cafb8d9ca950a64d8 100644
--- a/scripts/mod/modpost.c
+++ b/scripts/mod/modpost.c
@@ -1913,6 +1913,11 @@ static void check_exports(struct module *mod)
 		const char *basename;
 		exp = find_symbol(s->name);
 		if (!exp) {
+			/* ignore the unresolved symbols caused by only using
